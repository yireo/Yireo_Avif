<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Yireo\NextGenImages\Convertor\ConvertorListing">
        <arguments>
            <argument name="convertors" xsi:type="array">
                <item name="avif" xsi:type="object">Yireo\Avif\Convertor\Convertor</item>
            </argument>
        </arguments>
    </type>

    <virtualType name="Yireo\Avif\VirtualType\Block\Adminhtml\System\Config\ModuleVersion" type="Yireo\NextGenImages\Block\Adminhtml\System\Config\ModuleVersion">
        <arguments>
            <argument name="moduleName" xsi:type="string">Yireo_Avif</argument>
        </arguments>
    </virtualType>
</config>
